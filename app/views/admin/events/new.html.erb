New Event
<%= form_for [:admin, @event] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :start_at %>
  <%= f.datetime_select :start_at %>
  <%= f.submit %>
<% end %>
