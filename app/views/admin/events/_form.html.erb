<%= render 'shared/form_errors', object: @event %>
<%= form_for [:admin, @event] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :start_at %>
  <%= f.text_field :start_at, :class => "datepicker"%>
  <br />
  <%= f.label :end_at %>
  <%= f.text_field :end_at, :class => "datepicker"%>
  <br />
  <%= f.submit %>
<% end %>
